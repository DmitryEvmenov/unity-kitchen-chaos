# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/src/**UnityApp/[Ll]ibrary/
/src/**UnityApp/[Tt]emp/
/src/**UnityApp/[Oo]bj/
/src/**UnityApp/[Bb]uild/
/src/**UnityApp/[Bb]uilds/
/src/**UnityApp/[Ll]ogs/
/src/**UnityApp/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/src/**UnityApp/[Mm]emoryCaptures/

# Recordings can get excessive in size
/src/**UnityApp/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /src/**UnityApp/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/src/**UnityApp/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/src/**UnityApp/.vs/
/src/**UnityApp/.vsconfig

# Gradle cache directory
/src/**UnityApp/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/src/**UnityApp/ExportedObj/
/src/**UnityApp/.consulo/
/src/**UnityApp/*.csproj
/src/**UnityApp/*.unityproj
/src/**UnityApp/*.sln
/src/**UnityApp/*.suo
/src/**UnityApp/*.tmp
/src/**UnityApp/*.user
/src/**UnityApp/*.userprefs
/src/**UnityApp/*.pidb
/src/**UnityApp/*.booproj
/src/**UnityApp/*.svd
/src/**UnityApp/*.pdb
/src/**UnityApp/*.mdb
/src/**UnityApp/*.opendb
/src/**UnityApp/*.VC.db

# Unity3D generated meta files
*.pidb.meta
*.pdb.meta
*.mdb.meta

# Unity3D generated file on crash reports
/src/**UnityApp/sysinfo.txt

# Builds
*.apk
*.aab
*.unitypackage
*.app

# Crashlytics generated file
/src/**UnityApp/crashlytics-build.properties

# Packed Addressables
/src/**UnityApp/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/src/**UnityApp/[Aa]ssets/[Ss]treamingAssets/aa.meta
/src/**UnityApp/[Aa]ssets/[Ss]treamingAssets/aa/*